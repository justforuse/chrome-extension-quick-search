{
  "name": "Quick Search",
  "description": "Quick search in useful websites for web developer",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_popup": "popup.html",
    "default_title": "Quick Search"
  },
  "omnibox": {
    "keyword": "qs"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}
